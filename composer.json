{
  "name": "rexlabs/laravel-smokescreen",
  "keywords": [
    "laravel",
    "api",
    "transform",
    "json",
    "smokescreen",
    "model",
    "eloquent",
    "serializer",
    "fractal"
  ],
  "authors": [
    {
      "name": "Jodie Dunlop",
      "email": "jodie.dunlop@rexsoftware.com.au"
    }
  ],
  "require": {
    "php": ">=7.0",
    "rexlabs/smokescreen": "^2.0.0",
    "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
    "doctrine/dbal": "^2.5|^2.7"
  },
  "require-dev": {
    "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0",
    "phpunit/phpunit": "^6.5.7|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Rexlabs\\Laravel\\Smokescreen\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rexlabs\\Laravel\\Smokescreen\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rexlabs\\Laravel\\Smokescreen\\Providers\\ServiceProvider"
      ],
      "aliases": {
        "Smokescreen": "Rexlabs\\Laravel\\Smokescreen\\Facades\\Smokescreen"
      }
    }
  },
  "scripts": {
    "test": "phpunit",
    "tests": "phpunit --colors=always",
    "coverage": "phpunit --coverage-html ./tests/report"
  },
  "license": "MIT"
}
