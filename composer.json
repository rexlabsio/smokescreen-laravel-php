{
  "name": "rexlabs/laravel-smokescreen",
  "keywords": [
    "laravel",
    "api",
    "transform",
    "json",
    "smokescreen",
    "model",
    "eloquent",
    "serializer",
    "fractal"
  ],
  "authors": [
    {
      "name": "Jodie Dunlop",
      "email": "jodie.dunlop@rexsoftware.com.au"
    }
  ],
  "require": {
    "php": "^7.0",
    "rexlabs/smokescreen": "^1.1.0",
    "illuminate/contracts": "~5.4.0|~5.5.0|~5.6.0",
    "illuminate/database": "~5.4.0|~5.5.0|~5.6.0",
    "illuminate/http": "~5.4.0|~5.5.0|~5.6.0",
    "illuminate/pagination": "~5.4.0|~5.5.0|~5.6.0",
    "illuminate/support": "~5.4.0|~5.5.0|~5.6.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.4.0|~3.5.0|~3.6.0",
    "phpunit/phpunit": "^6.5.7|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Rexlabs\\Laravel\\Smokescreen\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rexlabs\\Laravel\\Smokescreen\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rexlabs\\Laravel\\Smokescreen\\Providers\\ServiceProvider"
      ],
      "aliases": {
        "Smokescreen": "Rexlabs\\Laravel\\Smokescreen\\Facades\\Smokescreen"
      }
    }
  },
  "scripts": {
    "test": "phpunit",
    "tests": "phpunit --colors=always",
    "coverage": "phpunit --coverage-html ./tests/report"
  },
  "license": "MIT"
}
