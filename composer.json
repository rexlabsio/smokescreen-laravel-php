{
  "name": "rexlabs/laravel-smokescreen",
  "keywords": [
    "laravel",
    "api",
    "transform",
    "json",
    "smokescreen",
    "model",
    "eloquent",
    "serializer",
    "fractal"
  ],
  "authors": [
    {
      "name": "Jodie Dunlop",
      "email": "jodie.dunlop@rexsoftware.com.au"
    }
  ],
  "require": {
    "php": ">=7.1",
    "rexlabs/smokescreen": "^2.0",
    "laravel/framework": "^5.6|^6.0|^7.0|^8.0",
    "doctrine/dbal": "^2.5|^2.7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "orchestra/testbench": "^3.0|^4.0|^5.0|^5.0",
    "phpunit/phpunit": "^6.5.7|^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Rexlabs\\Laravel\\Smokescreen\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rexlabs\\Laravel\\Smokescreen\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rexlabs\\Laravel\\Smokescreen\\Providers\\ServiceProvider"
      ],
      "aliases": {
        "Smokescreen": "Rexlabs\\Laravel\\Smokescreen\\Facades\\Smokescreen"
      }
    }
  },
  "scripts": {
    "test": "phpunit --color=always",
    "tests": "phpunit --color=always",
    "coverage": "phpunit --coverage-html ./tests/report"
  },
  "license": "MIT"
}
